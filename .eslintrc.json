{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "error",
    "prefer-const": "error",
    "no-var": "error",
    "one-var": "off",
    "vars-on-top": "off",
    "arrow-parens": "error",
    "camelcase": "error"
  }
}
